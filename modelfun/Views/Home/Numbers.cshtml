<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Numbers</title>
</head>

<body>
    @model int[]

    <div class="justify-content-center">
        <h4> Here are some Numbers</h4>
        @{
            foreach (int numbers in Model)
            {
                <p>@numbers</p>
            }
        }
    </div>

</body>

</html>