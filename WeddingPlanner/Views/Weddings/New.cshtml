@model Wedding
<h1>Plan your Wedding!</h1>
<div class="container">
    <form asp-action="Create" asp-controller="Weddings" method="POST">
        <div class="d-flex mb-4 mt-5">
            <span asp-validation-for="WedderOne"></span>
            <label asp-for="WedderOne" class="form-label"></label>
            <input class="form-control" asp-for="WedderOne">
        </div>
        <div class="d-flex mb-4">
            <span asp-validation-for="WedderTwo"></span>
            <label asp-for="WedderTwo" class="form-label"></label>
            <input class="form-control" asp-for="WedderTwo">
        </div>
        <div class="d-flex mb-4">
            <span asp-validation-for="Date"></span>
            <label asp-for="Date" class="form-label"></label>
            <input class="form-control" asp-for="Date" type="date">
        </div>
        <div>
            <span asp-validation-for="Address"></span>
            <label asp-for="Address"></label>
            <input class="form-control" asp-for="Address">
        </div>

        <button id="submit" class="btn btn-success mt-4" value="Create" type="submit">SUBMIT!</button>

    </form>
